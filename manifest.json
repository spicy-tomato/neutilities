{
  "manifest_version": 3,
  "name": "Neutilities",
  "version": "1.0.1",
  "description": "Essential Tools for NEU Students",
  "permissions": [
    "alarms",
    "offscreen",
    "storage"
  ],
  "host_permissions": [
    "https://daihocchinhquy.neu.edu.vn/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "16": "./icons/16.png",
    "32": "./icons/32.png",
    "48": "./icons/48.png",
    "128": "./icons/128.png"
  }
}
